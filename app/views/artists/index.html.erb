<h1>Artists!</h1>

<h3>Here are all of the artists!</h3>

<div class="row">
  <% @artists.each do |artist| %>
    <div class="col-md-3">
      <h3><%= link_to "#{artist.name}", artist_path(artist.id) %></h3>
      <h3><em>Genre: <%= link_to "#{artist.genre.name}", genre_path(artist.genre.id) %></em></h3>
    </div>
  <% end %>
</div>
